{
  "root": true,
  "env": {
      "node": true
  },
  "extends": [
      "plugin:vue/essential",
      "@vue/standard"
  ],
  "rules": {
      "no-eval": "warn",
      "code": 140,
      "quotes": [
          "error",
          "single"
      ],
      "curly": [
          "error",
          "multi-line"
      ],
      "comma-dangle": [
          "error",
          "always-multiline"
      ]
  },
  "parserOptions": {
      "parser": "babel-eslint"
  }
}